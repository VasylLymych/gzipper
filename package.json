{
  "name": "gzipper",
  "version": "2.7.0",
  "description": "CLI for compressing files.",
  "main": "index.js",
  "scripts": {
    "test": "nyc --check-coverage --reporter='text-summary' mocha --timeout=3000 --recursive './tests/**/*.spec.js'",
    "coverage": "nyc mocha --timeout=3000 --recursive './tests/**/*.spec.js'",
    "prettier": "prettier --write \"**/*.js\"",
    "eslint": "eslint \"**/*.js\"",
    "check": "npm run prettier && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gios/gzipper.git"
  },
  "bin": "./index.js",
  "keywords": [
    "tool",
    "cli",
    "zlib",
    "zip",
    "gzip",
    "script",
    "gzipper",
    "brotli",
    "angular",
    "compression"
  ],
  "author": "Gios",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gios/gzipper/issues"
  },
  "homepage": "https://github.com/gios/gzipper#readme",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^11.13.13",
    "@types/semver": "^6.0.0",
    "@types/sinon": "^7.0.12",
    "@types/uuid": "^3.4.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "semver": "^6.1.1",
    "sinon": "^7.3.2"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "uuid": "^3.3.2"
  }
}
